{
  "scripts": {
    "test1": "mocha tests/.setup.js tests/**/*-test.js --compilers css:mocha-compiler.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build": "rimraf ./dist && webpack --config ./build/webpack.prod.config.js --progress --colors",
    "mock": "cross-env MOCK=1 npm run dev",
    "dev": "node devServer",
    "genprotojs": "node ./build/gen-proto-js.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__mocks__/**",
      "!**/coverage/**",
      "!**/build/**",
      "!**/dist/**",
      "!**/api/**",
      "!**/network/**",
      "!**/assets/**",
      "!**/containers/**",
      "!**/middlewares/**",
      "!**/routes/**",
      "!**/selectors/**",
      "!**/mapping.js",
      "!**/store/**",
      "!**/utils/**",
      "!**/components/Router/**",
      "!**/reducers/index.js",
      "!**/src/index.jsx",
      "!**/devServer.js",
      "!**/postcss.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "dependencies": {
    "antd": "^3.3.0",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.14.1",
    "babel-polyfill": "^6.23.0",
    "enquire-js": "^0.1.3",
    "fastclick": "^1.0.6",
    "lodash-decorators": "^5.0.0",
    "path-to-regexp": "^2.2.0",
    "prop-types": "^15.6.1",
    "rc-drawer-menu": "^0.5.6",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.6.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-loader": "^2.0.0",
