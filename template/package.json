    "jest": "^22.4.2",
    "less": "^2.7.1",
    "less-loader": "^4.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-px2rem": "^0.3.0",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.2.3",
